                    IDC_STATIC,21,75,109,21,SS_SUNKEN,WS_EX_CLIENTEDGE
    LTEXT           "Version 0.3 (alpha)",IDC_STATIC,43,40,60,8
    LTEXT           "lnwalker@hotmail.com",IDC_STATIC,43,62,72,8
    LTEXT           "NW Debugger",IDC_STATIC,43,10,47,8
END

