    GROUPBOX        "Debug Messages",IDC_STATIC,9,40,228,144
    GROUPBOX        "Actions",IDC_STATIC,244,44,68,140,0,WS_EX_DLGMODALFRAME
    ICON            IDI_ICON1,IDC_STATIC,9,10,21,20,0,WS_EX_DLGMODALFRAME
