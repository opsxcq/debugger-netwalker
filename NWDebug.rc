    ICON            IDI_ICON1,IDC_STATIC,11,11,18,20,0,WS_EX_DLGMODALFRAME
    LTEXT           "Simple Debugger Model",IDC_STATIC,43,19,76,8
    LTEXT           "By Net Walker! ",IDC_STATIC,43,30,51,8
    LTEXT           "May, 14th -1998 - Brazil",IDC_STATIC,43,51,75,8
